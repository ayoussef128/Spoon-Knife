//create new repo in command line

echo "# first-repo" >> README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin https://github.com/ayoussef128/first-repo.git
git push -u origin master

//push an existing repo from command line

git remote add origin https://github.com/ayoussef128/first-repo.git
git push -u origin master
